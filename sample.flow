      },
      "s2": {
        "type": "kronos-copy",
        "endpoints": {
          "out": "step:s3/in",
          "log": "step:log/in"
        }
      },
      "s3": {
        "type": "kronos-copy",
        "endpoints": {
          "out": "step:s4/in",
