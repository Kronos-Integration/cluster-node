{
  "name": "flow1",
  "description": "the flow description 1",
  "steps": {
    "s1": {
      "type": "kronos-stdin",
      "endpoints": {
        "out": "s2/in",
        "log": "log/in"
      }
    },
    "s2": {
      "type": "kronos-copy",
      "endpoints": {
        "out": "s3/in",
        "log": "log/in"
      }
    },
    "s3": {
      "type": "kronos-copy",
      "endpoints": {
        "out": "s4/in",
        "log": "log/in"
      }
    },
    "s4": {
      "type": "kronos-stdout",
      "endpoints": {
        "log": "log/in"
      }
    },
    "log": {
      "type": "kronos-stderr"
    }
  }
}
