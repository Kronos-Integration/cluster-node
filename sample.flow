{
  "name": "post-demo",
  "type": "kronos-flow",
  "description": "http post demo",
  "steps": {
    "s1": {
      "type": "kronos-http-routing",
      "listener": {
        "name": "demo",
        "port": 12345
      },
      "routes": {
        "in": {
          "path": "/in",
          "method": "POST",
          "target": "s2/in",
          "interceptors" : [
            { "type": "timeout", "timeout" : 2000 },
            { "type": "limit", "limit" : 10 }
          ]
        }
      }
    },
    "s2": {
      "type": "kronos-stdout",
      "endpoints": {
        "log": "log/in"
      }
    },
    "log": {
      "type": "kronos-stderr"
    }
  }
}
