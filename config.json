[{
  "name": "kronos"
}, {
  "name": "koa-admin",
  "listen": {
    "retryTimeout": 600,
    "address": "0.0.0.0",
    "fromPort": 10001,
    "toPort": "${fromPort + 8}"
  },
  "docRoot": "${directory('docroot')}"
}, {
  "name": "admin",
  "ssh": {
    "hostKeys": [
      "${file('etc/hostKey')}"
    ],
    "port": 10010
  }
}, {
  "name": "registry",
  "checkInterval": 60,
  "port": 8500,
  "host": "localhost"
}, {
  "name": "health-check",
  "memoryInterval": 60,
  "uptimeInterval": 60
}]
