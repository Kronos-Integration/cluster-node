{
  "name": "kronos-cluster-node",
  "bin": "bin/cluster_node",
  "version": "0.0.0-semantic-release",
  "description": "kronos cluster managed node sample application",
  "scripts": {
    "start": "node bin/cluster_node",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "node": ">=5"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "commander": "2.9.0",
    "kronos-adapter-inbound-file": "2.0.0",
    "kronos-adapter-outbound-file": "2.0.0",
    "kronos-flow": "2.0.0",
    "kronos-flow-control-step": "2.0.0",
    "kronos-http-routing-step": "3.0.0",
    "kronos-interceptor-line-header": "2.0.1",
    "kronos-interceptor-line-parser": "2.0.0",
    "kronos-interceptor-line-tokenizer-csv": "2.0.1",
    "kronos-interceptor-line-tokens2obj": "2.0.0",
    "kronos-interceptor-message-handler": "2.0.0",
    "kronos-service-admin": "2.0.2",
    "kronos-service-consul": "2.0.3",
    "kronos-service-health-check": "2.0.2",
    "kronos-service-manager": "^3.2.1",
    "kronos-service-uti": "1.0.0",
    "kronos-step-archive-tar": "2.0.0",
    "kronos-step-gzip": "^2.0.0",
    "kronos-step-passthrough": "2.0.0",
    "kronos-step-stdio": "3.0.0",
    "kronos-step-system": "2.0.0",
    "list_files": "0.1.1",
    "pkginfo": "0.3.1",
    "rebirth": "0.2.2",
    "systemd-socket": "0.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "mocha": "2.4.5",
    "semantic-release": "6.2.0",
    "cz-conventional-changelog": "1.1.5"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
