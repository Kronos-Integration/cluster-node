{
  "name": "kronos-cluster-node",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "bin/kronos-cluster-node",
  "module": "src/kronos-cluster-node.js",
  "description": "kronos cluster node sample application",
  "keywords": [
    "cluster"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "kronos-cluster-node": "bin/kronos-cluster-node"
  },
  "scripts": {
    "start": "node bin/kronos-cluster-node",
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/kronos-cluster-node.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/kronos-cluster-node.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "config-expander": "9.1.6",
    "kronos-adapter-inbound-file": "2.0.15",
    "kronos-adapter-outbound-file": "2.0.14",
    "kronos-adapter-outbound-http": "1.0.13",
    "kronos-flow": "^2.5.34",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "1.0.12",
    "kronos-interceptor-http-request": "3.2.10",
    "kronos-interceptor-line-header": "^2.1.9",
    "kronos-interceptor-line-parser": "2.0.10",
    "kronos-interceptor-line-tokenizer-csv": "2.1.7",
    "kronos-interceptor-line-tokens2obj": "^2.1.8",
    "kronos-interceptor-message-handler": "2.1.5",
    "kronos-interceptor-object-data-processor-chunk": "^2.1.8",
    "kronos-interceptor-object-data-processor-row": "^1.2.11",
    "kronos-interceptor-stream-obj2string": "2.0.14",
    "kronos-service-admin": "^2.26.22",
    "kronos-service-consul": "2.20.0",
    "kronos-service-health-check": "2.11.0",
    "kronos-service-manager": "3.5.10",
    "kronos-service-uti": "1.2.0",
    "kronos-step-aggregate": "^1.4.6",
    "kronos-step-archive-tar": "2.0.13",
    "kronos-step-file": "^1.0.19",
    "kronos-step-gzip": "^2.0.13",
    "kronos-step-passthrough": "^2.2.0",
    "kronos-step-stdio": "^3.0.18",
    "kronos-step-system": "^2.0.10",
    "network-address": "^1.1.2",
    "npm-package-walker": "^3.0.4",
    "rebirth": "^2.0.0",
    "systemd-socket": "^0.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^8.0.1",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-executable": "^1.2.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.7.2",
    "travis-deploy-once": "^5.0.1"
  },
  "engines": {
    "node": ">=10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-cluster-node#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
