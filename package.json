{
  "name": "kronos-cluster-node",
  "bin": {
    "kronos-cluster-node": "bin/kronos-cluster-node"
  },
  "version": "0.0.0-semantic-release",
  "description": "kronos cluster node sample application",
  "module": "src/cluster_node.js",
  "main": "bin/cluster_node",
  "scripts": {
    "start": "node bin/cluster_node",
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=bin/kronos-cluster-node -c -- src/cluster_node.js && chmod +x bin/kronos-cluster-node",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "engines": {
    "node": ">=8.1.3"
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "caporal": "^0.7.0",
    "config-expander": "7.1.2",
    "glob": "^7.1.1",
    "kronos-adapter-inbound-file": "^2.0.14",
    "kronos-adapter-outbound-file": "^2.0.12",
    "kronos-adapter-outbound-http": "^1.0.11",
    "kronos-flow": "^2.5.34",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "1.0.11",
    "kronos-interceptor-http-request": "3.2.8",
    "kronos-interceptor-line-header": "^2.1.6",
    "kronos-interceptor-line-parser": "2.0.10",
    "kronos-interceptor-line-tokenizer-csv": "2.1.7",
    "kronos-interceptor-line-tokens2obj": "2.1.5",
    "kronos-interceptor-message-handler": "2.1.5",
    "kronos-interceptor-object-data-processor-chunk": "^2.1.5",
    "kronos-interceptor-object-data-processor-row": "^1.2.9",
    "kronos-interceptor-stream-obj2string": "2.0.14",
    "kronos-service-admin": "2.26.10",
    "kronos-service-consul": "2.19.9",
    "kronos-service-health-check": "2.10.11",
    "kronos-service-manager": "3.5.10",
    "kronos-service-uti": "1.1.8",
    "kronos-step-aggregate": "^1.4.6",
    "kronos-step-archive-tar": "^2.0.11",
    "kronos-step-file": "^1.0.19",
    "kronos-step-gzip": "^2.0.13",
    "kronos-step-passthrough": "^2.0.15",
    "kronos-step-stdio": "^3.0.18",
    "kronos-step-system": "^2.0.10",
    "network-address": "^1.1.2",
    "rebirth": "^0.2.3",
    "systemd-socket": "^0.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rollup": "^0.45.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-release": "^6.3.6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-cluster-node#readme",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}