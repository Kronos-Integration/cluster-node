{
  "name": "kronos-cluster-node",
  "bin": "bin/cluster_node",
  "version": "0.0.0",
  "description": "kronos cluster managed node",
  "scripts": {
    "start": "node bin/cluster_node",
    "cover": "node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha --harmony test/*.js",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "engines": {
    "node": ">=4.2.1"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "commander": "2.9.0",
    "koa-static": "1.5.2",
    "kronos-flow": "1.3.1",
    "kronos-service-manager": "2.3.0",
    "kronos-service-manager-admin": "1.1.4",
    "kronos-service-manager-consul": "1.2.2",
    "kronos-step-archive-tar": "^1.0.0",
    "kronos-step-stdio": "^2.0.0",
    "kronos-step-system": "1.1.0",
    "pkginfo": "^0.3.1",
    "rebirth": "^0.2.2",
    "systemd-socket": "0.0.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "3.4.0",
    "mocha": "2.3.4",
    "semantic-release": "6.0.3"
  }
}
