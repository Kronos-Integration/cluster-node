    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "node": ">=5"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "commander": "2.9.0",
    "expression-expander": "^3.0.0",
    "glob": "^7.0.3",
    "kronos-adapter-inbound-file": "2.0.1",
    "kronos-adapter-outbound-file": "2.0.1",
    "kronos-adapter-outbound-http": "1.0.2",
    "kronos-flow": "2.5.10",
    "kronos-flow-control-step": "2.2.1",
