{
  "name": "kronos-cluster-node",
  "bin": {
    "kronos-cluster-node": "bin/kronos-cluster-node"
  },
  "version": "0.0.0-semantic-release",
  "description": "kronos cluster node sample application",
  "module": "src/kronos-cluster-node.js",
  "main": "bin/kronos-cluster-node",
  "scripts": {
    "start": "node bin/kronos-cluster-node",
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c && chmod +x bin/kronos-cluster-node"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "caporal": "^0.8.0",
    "config-expander": "9.0.1",
    "kronos-adapter-inbound-file": "2.0.15",
    "kronos-adapter-outbound-file": "2.0.14",
    "kronos-adapter-outbound-http": "1.0.13",
    "kronos-flow": "^2.5.34",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "1.0.12",
    "kronos-interceptor-http-request": "3.2.9",
    "kronos-interceptor-line-header": "^2.1.8",
    "kronos-interceptor-line-parser": "2.0.10",
    "kronos-interceptor-line-tokenizer-csv": "2.1.7",
    "kronos-interceptor-line-tokens2obj": "^2.1.7",
    "kronos-interceptor-message-handler": "2.1.5",
    "kronos-interceptor-object-data-processor-chunk": "^2.1.7",
    "kronos-interceptor-object-data-processor-row": "^1.2.10",
    "kronos-interceptor-stream-obj2string": "2.0.14",
    "kronos-service-admin": "2.26.20",
    "kronos-service-consul": "2.19.12",
    "kronos-service-health-check": "2.10.11",
    "kronos-service-manager": "3.5.10",
    "kronos-service-uti": "1.1.12",
    "kronos-step-aggregate": "^1.4.6",
    "kronos-step-archive-tar": "2.0.13",
    "kronos-step-file": "^1.0.19",
    "kronos-step-gzip": "^2.0.13",
    "kronos-step-passthrough": "^2.0.15",
    "kronos-step-stdio": "^3.0.18",
    "kronos-step-system": "^2.0.10",
    "network-address": "^1.1.2",
    "npm-package-walker": "^3.0.1",
    "rebirth": "^1.0.2",
    "systemd-socket": "^0.0.0"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.3.0",
    "rollup": "^0.51.5",
    "semantic-release": "^9.1.1",
    "xo": "^0.19.0",
    "babel-preset-env": "^1.6.1"
  },
  "keywords": [
    "cluster"
  ],
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-cluster-node#readme",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
