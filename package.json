{
  "name": "kronos-cluster-node",
  "bin": "bin/cluster_node",
  "version": "0.0.0",
  "description": "kronos cluster managed node",
  "scripts": {
    "start": "node bin/cluster_node",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha tests/*.js",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "engines": {
    "node": ">=4.2.1"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node.git"
  },
  "dependencies": {
    "commander": "2.9.0",
    "kronos-flow": "^1.5.0",
    "kronos-flow-control-step": "^1.6.0",
    "kronos-health-check-service": "^1.0.1",
    "kronos-http-routing-step": "^2.6.0",
    "kronos-service-consul": "1.9.0",
    "kronos-service-manager": "^2.13.1",
    "kronos-service-manager-admin": "git+https://github.com/Kronos-Integration/kronos-service-manager-admin.git",
    "kronos-step-archive-tar": "^1.4.1",
    "kronos-step-passthrough": "^1.1.4",
    "kronos-step-stdio": "^2.1.0",
    "kronos-step-system": "^1.5.0",
    "list_files": "^0.1.1",
    "pkginfo": "^0.3.1",
    "rebirth": "^0.2.2",
    "systemd-socket": "0.0.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "3.4.0",
    "mocha": "2.3.4",
    "semantic-release": "6.0.3"
  }
}
