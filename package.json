{
  "name": "kronos-cluster-node",
  "bin": "bin/cluster_node",
  "version": "0.0.0",
  "description": "kronos cluster managed node",
  "scripts": {
    "start": "node bin/cluster_node",
    "cover": "node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha --harmony test/*.js",
    "ci-test-report": "mocha --harmony --reporter mocha-html-reporter test/*.js",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "engines": {
    "node": ">=4.2.1"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "ssh://donkey@mfelten.de/Groups/donkey/git/kronos-cluster-node.git"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "koa-static": "^1.4.9",
    "kronos-service-manager-consul": "https://github.com/Kronos-Integration/kronos-service-manager-consul.git",
    "kronos-service-manager": "^1.0.4",
    "kronos-step-stdio": "https://github.com/Kronos-Integration/kronos-step-stdio.git",
    "kronos-service-manager-rest": "git+ssh://donkey@mfelten.de/Groups/donkey/git/kronos-service-manager-rest.git",
    "kronos-step-archive-tar": "https://github.com/Kronos-Integration/kronos-step-archive-tar.git",
    "rebirth": "^0.2.2",
    "systemd-socket": "0.0.0"
  },
  "devDependencies": {
    "chai": "3.2.0",
    "istanbul": "0.4.0",
    "jsdoc": "^3.3.2",
    "mocha": "^2.3.2",
    "mocha-html-reporter": "^0.0.1",
    "semantic-release": "^6.0.1"
  }
}
