    "rebirth": "0.2.3",
    "systemd-socket": "0.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.1.5",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "mocha": "2.4.5",
    "semantic-release": "6.2.1"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-cluster-node/issues"
